{
  "version": 2,
  "builds": [
    {
      "src": "CineList/wsgi.py",
      "use": "@vercel/python",
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ui/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "CineList/wsgi.py"
    }
  ]
}
